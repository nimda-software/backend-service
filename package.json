{
  "name": "@megrulad-ge/backend-service",
  "version": "0.0.3",
  "description": "Backend service for the application",
  "author": "n1md7",
  "private": false,
  "bin": {
    "backend-service": "dist/main.js"
  },
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "prestart:prod": "npm run migrations",
    "start:prod": "cross-env NODE_ENV=production node --max-http-header-size=32768 dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --force-exit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "gen:env-files": "node scripts/gen-envs.cjs",
    "gen:env-types": "npx gen-env-types .env.example -o src/env.d.ts -k -e .",
    "gen:docs": "npx compodoc -p tsconfig.doc.json --output public",
    "gen:db-diagram": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm-uml src/__common/setup/config/uml-diagram.config.ts -d ../../../../docs/db-diagram.svg -f svg --with-enum-values",
    "typeorm": "cross-env NODE_ENV=development typeorm-ts-node-esm --dataSource src/__common/setup/config/migration.config.ts",
    "migrations": "npm run typeorm -- migration:run",
    "migration:create": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:create",
    "migration:generate": "npm run typeorm -- migration:generate --pretty",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/common-v8": "npm:@nestjs/common@^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/core-v8": "npm:@nestjs/core@^8.4.7",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/platform-express-v8": "npm:@nestjs/platform-express@^8.4.7",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^6.19.7",
    "@sentry/types": "^6.19.7",
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^4.1.1",
    "dotenv": "^10.0.0",
    "eventemitter2": "^6.4.6",
    "joi": "^17.6.0",
    "keycloak-connect": "^15.1.0",
    "ms": "^2.1.2",
    "nest-keycloak-connect": "^1.9.0",
    "node-cache": "^5.1.2",
    "pg": "^8.7.3",
    "prom-client": "^14.0.1",
    "ramda": "^0.27.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^9.0.5",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/ramda": "^0.27.66",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "axios-mock-adapter": "^1.21.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gen-env-types": "^1.3.4",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-junit": "^13.2.0",
    "js-yaml": "^3.14.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "request": "^2.88.2",
    "sqlite3": "^5.0.10",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typeorm-uml": "^1.6.5",
    "typeorm-v2": "npm:typeorm@^0.2.45",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "index.ts",
      "main.ts",
      "cli.ts",
      "src/__common/bootstrap/.*",
      "src/__common/setup/seeder/.*",
      "migration.config.ts",
      "uml-diagram.config.ts",
      ".*\\.d\\.ts$",
      ".*\\.interceptor\\.ts$",
      ".*\\.module\\.ts$",
      ".*\\.decorator\\.ts$",
      ".*\\.type\\.ts$",
      ".*\\.mock\\.ts$",
      ".*\\.enum\\.ts$",
      ".*\\.schema\\.ts$"
    ],
    "coverageDirectory": "../reports/coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "jest tests",
          "outputDirectory": "reports",
          "outputName": "junit.xml",
          "uniqueOutputName": "false",
          "classNameTemplate": "{classname}-{title}",
          "titleTemplate": "{classname}-{title}",
          "ancestorSeparator": " â€º ",
          "usePathForSuiteName": "true"
        }
      ]
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "clover",
      "cobertura"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=18.6.0",
    "npm": ">=8.13.2"
  },
  "engineStrict": true
}
