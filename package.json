{
  "name": "@megrulad-ge/backend-service",
  "version": "0.0.3",
  "description": "Backend service for the application",
  "author": "n1md7",
  "private": false,
  "bin": {
    "backend-service": "dist/main.js"
  },
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "prestart:prod": "npm run migrations",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --force-exit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "gen:env-files": "node scripts/gen-envs.cjs",
    "gen:env-types": "npx gen-env-types .env.example -o src/env.d.ts -k -e .",
    "gen:docs": "npx @compodoc/compodoc -p tsconfig.doc.json --output public",
    "typeorm": "cross-env NODE_ENV=development typeorm-ts-node-commonjs --dataSource src/common/setup/config/migration.config.ts",
    "migrations": "npm run typeorm -- migration:run",
    "migration:create": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:create",
    "premigration:generate": "npm run build",
    "migration:generate": "npm run typeorm -- migration:generate --pretty",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/passport-strategy": "^0.2.37",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "mysql2": "^3.9.7",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.22",
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/bcrypt": "^5.0.1",
    "@types/cache-manager": "^4.0.4",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/ms": "^0.7.33",
    "@types/node": "^20.8.9",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.6",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "chalk": "^4.1.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gen-env-types": "^1.3.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "3.0.3",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^/src/(.*)$": "<rootDir>/../src/$1",
      "^/common/(.*)$": "<rootDir>/../src/common/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "index.ts",
      "main.ts",
      "src/common/bootstrap/.*",
      "src/common/setup/seeder/.*",
      "src/common/setup/config/.*",
      "src/common/exceptions/.*",
      ".*\\.d\\.ts$",
      ".*\\.interceptor\\.ts$",
      ".*\\.module\\.ts$",
      ".*\\.decorator\\.ts$",
      ".*\\.type\\.ts$",
      ".*\\.mock\\.ts$",
      ".*\\.enum\\.ts$",
      ".*\\.schema\\.ts$"
    ],
    "coverageDirectory": "../reports/coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "jest tests",
          "outputDirectory": "reports",
          "outputName": "junit.xml",
          "uniqueOutputName": "false",
          "classNameTemplate": "{classname}-{title}",
          "titleTemplate": "{classname}-{title}",
          "ancestorSeparator": " â€º ",
          "usePathForSuiteName": "true"
        }
      ]
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "clover",
      "cobertura"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "engineStrict": true
}
